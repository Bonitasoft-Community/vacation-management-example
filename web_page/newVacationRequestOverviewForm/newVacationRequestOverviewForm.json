  "inactiveAssets" : [ ],
  "variables" : {
    "archivedCase" : {
      "type" : "url",
      "value" : [
        "../API/bpm/archivedCase?c=1&d=started_by&d=processDefinitionId&f=sourceObjectId%3D{{caseId}}&p=0"
      ],
      "exposed" : false
    },
    "caseOverview" : {
      "type" : "expression",
      "value" : [
        "function values(object){",
        "    if(!object){",
        "        return [];",
        "    }",
        "    return  Object.keys(object).map(function(key){",
        "        return $data.context[key];",
        "    });    ",
        "}",
        "",
        "function groupBy(xs, key) {",
        "  var result = xs.reduce(function(rv, x) {",
        "    (rv[x[key]] = rv[x[key]] || []).push(x);",
        "    return rv;",
        "  }, {});",
        "  return result ? Object.keys(result).map(function(key) { return { \"name\" : key, \"description\" : result[key] && result[key].length > 0 ? result[key][0].description : null  , \"documents\" : Array.isArray(result[key]) ?  result[key] : [ result[key]] }}) : [];",
        "}",
        "",
        "",
        "function maxValue(xs, key) {",
        "  return xs.reduce(function(prev, current) {",
        "       return (parseInt(prev[key],10) > parseInt(current[key],10)) ? prev : current;",
        "  });",
        "}",
        "",
        "var businessDataRefs =  values($data.context).filter(function(ref){",
        "        return ref && ref.type;",
        "    }).map(function(businessDataRef){",
        "        return {",
        "          \"name\" : businessDataRef.name + \" (\"+ businessDataRef.type +\")\",",
        "          \"link\" : businessDataRef.link",
        "        };",
        "    });",
        "",
        "var documentRefs = groupBy($data.openedContext ? $data.openedCaseDocuments : $data.archivedDocuments,\"name\");",
        "for(var index = 0; index < documentRefs.length; index++){",
        "          var doc = documentRefs[index];",
        "          var allVersionsById = groupBy(doc.documents, $data.openedContext ? \"id\" : \"sourceObjectId\");",
        "          for(var j = 0; j < allVersionsById.length; j++){",
        "                allVersionsById[j] = maxValue(allVersionsById[j].documents,\"version\");",
        "          }",
        "          doc.documents  = Object.values(allVersionsById);",
        "          if(doc.name + \"_ref\" in $data.context){",
        "              var contextDocs = $data.context[doc.name + \"_ref\"];",
        "              //Do not display removed docs form a multiple document",
        "              if(contextDocs && Array.isArray(contextDocs)){",
        "                  var docIdInContext = contextDocs.map(function(d) { return d.id.toString(); });",
        "                  doc.documents = doc.documents.filter(function(d) { return docIdInContext.includes(d.sourceObjectId ? d.sourceObjectId : d.id); });",
        "              }else if(!contextDocs){",
        "                  doc.documents = [];",
        "              }",
        "          }",
        " }",
        "",
        "return {",
        "    \"found\" : !!$data.case,",
        "    \"id\" : $data.case.id,",
        "    \"sourceObjectId\" : $data.case.rootCaseId ?  $data.case.rootCaseId  : $data.case.id ,",
        "    \"processName\" : $data.case.processDefinitionId.displayName + \" (\"+ $data.case.processDefinitionId.version+\")\",",
        "    \"businessDataRefs\" : businessDataRefs,",
        "    \"selectedBusinessDataRef\" : businessDataRefs[0],",
        "    \"documentRefs\" : documentRefs ",
        "};"
      ],
      "exposed" : false
    },
    "timelineOrigin" : {
      "type" : "expression",
      "value" : [
        "return {",
        " \"title\": \"Case started\",",
        " \"startTime\": $data.case.start,",
        " \"description\":$data.case.processDefinitionId.displayDescription,",
        " \"initiator\": $data.case.started_by.firstname + \" \" + $data.case.started_by.lastname",
        "};"
      ],
      "exposed" : false
    },
    "archivedTasks" : {
      "type" : "url",
      "value" : [
        "../API/bpm/archivedHumanTask?p=0&c=999&d=executedBy&f=caseId%3D{{caseOverview.sourceObjectId}}&o=reached_state_date DESC"
      ],
      "exposed" : false
    },
    "archivedContext" : {
      "type" : "url",
      "value" : [
        "../API/bpm/archivedCase/{{case.id}}/context"
      ],
      "exposed" : false
    },
    "selectedBusinessData" : {
      "type" : "url",
      "value" : [
        "../{{caseOverview.selectedBusinessDataRef.link}}"
      ],
      "exposed" : false
    },
    "openedContext" : {
      "type" : "url",
      "value" : [
        "../API/bpm/case/{{caseId}}/context"
      ],
      "exposed" : false
    },
    "timelineEvents" : {
      "type" : "expression",
      "value" : [
        "function executedBy(task){",
        "    return \"completed\" === task.state ? \"Executed by \" + task.executedBy.firstname + \" \" + task.executedBy.lastname : null;",
        "}",
        "",
        "function assignedTo(task){",
        "    return task.assigned_id ? \"Assigned to \" +  task.assigned_id.firstname + \" \" +  task.assigned_id.lastname : null;",
        "}",
        "",
        "function iconFor(task){",
        "    switch(task.state) {",
        "        case \"completed\": return \"check\";",
        "        case \"failed\": return \"remove-circle\";",
        "        case \"cancelled\": return \"ban-circle\";",
        "        case \"aborted\": return \"ban-circle\";",
        "        default: return \"unchecked\";",
        "    }",
        "}",
        "",
        "function styleFor(task){",
        "    switch(task.state) {",
        "        case \"completed\": return \"success\";",
        "        case \"failed\": return \"danger\";",
        "        case \"cancelled\": return \"warning\";",
        "        case \"aborted\": return \"warning\";",
        "        default: return \"primary\";",
        "    }",
        "}",
        "",
        "function toEvent(task){",
        "    return {",
        "      \"title\" : task.displayName,",
        "      \"lastUpdateDate\" : task.last_update_date,",
        "      \"executedBy\" :  executedBy(task),",
        "      \"assignedTo\" : assignedTo(task),",
        "      \"description\" : task.displayDescription,",
        "      \"icon\" : iconFor(task),",
        "      \"style\" : styleFor(task)",
        "    };",
        "}",
        "",
        "var archived = $data.archivedTasks.map(toEvent);",
        "var opened = $data.openedTasks.map(toEvent);",
        "",
        "return [].concat(opened,archived);"
      ],
      "exposed" : false
    },
    "openedTasks" : {
      "type" : "url",
      "value" : [
        "../API/bpm/task?p=0&c=999&d=executedBy&d=assigned_id&f=caseId%3D{{caseOverview.sourceObjectId}}&f=type%3DUSER_TASK&o=last_update_date DESC"
      ],
      "exposed" : false
    },
    "caseId" : {
      "type" : "urlparameter",
      "value" : [
        "id"
      ],
      "exposed" : false
    },
    "context" : {
      "type" : "expression",
      "value" : [
        "return (!$data.openCase && $data.archivedContext) ? $data.archivedContext : $data.openedContext;"
      ],
      "exposed" : false
    },
    "openCase" : {
      "type" : "url",
      "value" : [
        "../API/bpm/case/{{caseId}}?d=started_by&d=processDefinitionId"
      ],
      "exposed" : false
    },
    "businessDataKeys" : {
      "type" : "expression",
      "value" : [
        "/* Identifies if the passed key is a string version of an object id,",
        " * as the Rest API duplicates the object ids with \"_string\" version",
        " */",
        "function isOjectIdAsString(header, headers) {",
        "    return header.endsWith(\"_string\") && headers.includes(header.replace(\"_string\", \"\"));",
        "}",
        "",
        "var businessData = $data.selectedBusinessData.length ? $data.selectedBusinessData[0] :  $data.selectedBusinessData;",
        "",
        "return Object.keys(businessData).filter(function(header, index, headers){",
        "    return  !isOjectIdAsString(header, headers);",
        "}).filter(function(header){",
        "    return  header !== \"persistenceId\"",
        "            && header.indexOf(\"persistenceVersion\") === -1 ",
        "            && header !== \"links\" ",
        "});"
      ],
      "exposed" : false
    },
    "case" : {
      "type" : "expression",
      "value" : [
        "return  (!$data.openCase && $data.archivedCase.length > 0) ? $data.archivedCase[0] : $data.openCase;"
      ],
      "exposed" : false
    }
  },
